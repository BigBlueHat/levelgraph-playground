    "vuex": "^2.3.0"
  },
  "browser": {
    "vue": "vue/dist/vue.common"
  },
  "devDependencies": {
    "babel-core": "^6.22.1",
    "babel-preset-es2015": "^6.22.0",
    "babelify": "^7.3.0",
    "browserify": "^14.3.0",
    "http-server": "^0.10.0",
    "node-forge": "^0.7.1",
    "npm-run-all": "^4.0.2",
    "vueify": "^9.4.0",
    "watchify": "^3.9.0"
  },
  "browserify": {
    "transform": [
      "babelify",
      "vueify"
    ]
